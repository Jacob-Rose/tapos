{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "./js/generated",
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "types": ["node"]
  },
  "include": ["ts/**/*"],
  "exclude": ["node_modules"]
}
